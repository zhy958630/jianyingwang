@charset 'utf-8';
/**
    * 其他适用颜色
 */
.picture-shade {
  position: absolute;
  color: ;
  width: 100%;
  height: 100%;
  background: -webkit-radial-gradient(50% 0%, circle, transparent, rgba(0, 0, 0, 0.6) 60%);
  top: 0;
  left: 0;
  z-index: 10;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
  transform-origin: 50% 0%;
}
@media only screen and (max-width: 640px) {
  .picture-shade {
    display: none;
  }
}
.picture-shade:hover {
  background: -webkit-radial-gradient(50% 0%, circle, transparent, rgba(0, 0, 0, 0.3) 50%);
  transform: scale(2.5, 2.5);
}
.more {
  float: right;
  cursor: pointer;
}
.theTop-nav {
  background-color: rgba(255, 255, 255, 0.6);
}
@media only screen and (max-width: 640px) {
  .theTop-nav {
    position: absolute;
    width: 100%;
    top: 0;
    z-index: 100;
  }
}
.fixed-back {
  position: fixed;
  top: 0;
  z-index: -100;
}
.fixed-back > img {
  width: 100%;
  min-width: 900px;
  height: 310px;
}
@media only screen and (min-width: 1000px) {
  .fixed-back > img {
    height: 400px;
  }
}
@media only screen and (max-width: 640px) {
  .fixed-back > img {
    margin-left: -30%;
  }
}
.aminateText {
  top: -85px;
  left: 15%;
  position: absolute;
  z-index: 10;
}
@media only screen and (min-width: 641px) and (max-width: 999px) {
  .aminateText {
    top: -160px;
    right: 0;
    color: white;
  }
}
@-webkit-keyframes textOpacity {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@-webkit-keyframes textOne {
  from {
    transform: translateX(0);
    opacity: 0;
  }
  to {
    transform: translateX(-40px);
    opacity: 1;
  }
}
@-webkit-keyframes textTwo {
  from {
    transform: translateX(0);
    opacity: 0;
  }
  to {
    transform: translateX(40px);
    opacity: 1;
  }
}
.aminateText span {
  display: block;
  font-family: "宋体";
  font-size: 80px;
  line-height: 80px;
  color: rgba(0, 0, 0, 0.5);
  opacity: 1;
  text-shadow: 1px 1px 4px rgba(0, 0, 0, 0.3);
}
.aminateText .text-one {
  -webkit-animation: textOne 1s ease ;
  transform: translateX(-40px);
}
.aminateText .text-two {
  -webkit-animation: textTwo 1s ease ;
  transform: translateX(40px);
}
.content-back {
  position: relative;
  width: 100%;
  margin-top: 250px;
  -webkit-box-shadow: 0 -2px 8px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0 -2px 8px rgba(0, 0, 0, 0.2);
  box-shadow: 0 -2px 8px rgba(0, 0, 0, 0.2);
}
.content-back .picture-part {
  padding: 0 10%;
  background-color: #f8f8f8;
  padding-bottom: 20px;
  background-image: url(../img/lisi.png);
  background-repeat: repeat-x;
}
.content-back .picture-part .recommend {
  line-height: 25px;
  text-align: right;
  padding: 40px 10%;
}
.content-back .picture-part .recommend span {
  font-size: 18px ;
  font-family: "微软雅黑";
  color: #ff5a5a;
  padding: 4px 4px;
}
.content-back .picture-part .recommend a {
  font-size: 18px ;
  font-family: "微软雅黑";
  color: gray;
  padding: 4px 10px;
  display: inline-block;
  -webkit-transition: all 0.75s ease;
  -moz-transition: all 0.75s ease;
  -o-transition: all 0.75s ease;
  transition: all 0.75s ease;
}
.content-back .picture-part .recommend a:hover {
  color: #7d0022;
}
.content-back .picture-part-left {
  margin-top: 60px;
  padding-top: 4px;
  padding-bottom: 4px;
}
@media only screen and (max-width: 640px) {
  .content-back .picture-part-left {
    margin-top: -40px;
    -webkit-box-shadow: 0 0 4px rgba(0, 0, 0, 0.4);
    -moz-box-shadow: 0 0 4px rgba(0, 0, 0, 0.4);
    box-shadow: 0 0 4px rgba(0, 0, 0, 0.4);
  }
}
@media only screen and (max-width: 640px) {
  .content-back .picture-part-right {
    margin-top: 10px;
  }
}
.content-back .picture-part-right .row {
  padding: 0 11px;
}
.content-back .picture-part-right .row .title {
  height: 60px;
  line-height: 60px;
  padding-left: 30px;
  color: black;
  font-size: 25px;
}
.content-back .picture-part-right .row .title span {
  vertical-align: middle;
  padding-right: 10px ;
  color: #6452a0;
  margin-top: -5px;
}
.content-back .picture-part-right img {
  width: 100%;
}
.content-back .picture-part-right .img-div {
  padding: 0px;
  cursor: pointer;
}
.scaleImg {
  transform: scale(1, 1);
}
.scaleImgBig {
  transform: scale(1.5, 1.5);
}
.show-part {
  position: relative;
  overflow: hidden;
  background-color: white;
  -webkit-box-shadow: 0 -1px 4px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 0 -1px 4px rgba(0, 0, 0, 0.3);
  box-shadow: 0 -1px 4px rgba(0, 0, 0, 0.3);
}
.show-part hr {
  border-top: 2px #6452a0 solid;
  width: 0px;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.show-part .show-part-fixed {
  position: absolute;
  min-width: 1360px;
  top: 0px;
}
.show-part .show-part-fixed img {
  margin-top: 50px;
  width: 100%;
  transition: all 10s ease;
}
.show-part .title-part {
  width: 100%;
  padding: 0 10%;
  padding-top: 10px;
  position: relative;
  z-index: 2;
  background-color: #f8f8f8;
}
.show-part .title-part > p {
  text-align: center;
  font-family: "微软雅黑";
}
.show-part .title-part .title {
  font-size: 36px;
  line-height: 40px;
}
.show-part .title-part .small-title {
  font-size: 20px ;
  color: #7d0022;
  border-bottom: 2px solid #7d0022;
}
.show-part .title-part .glyphicon {
  display: block;
  font-size: 20px;
  height: 20px;
  color: #7d0022;
  position: absolute;
  left: 50%;
  top: auto;
  bottom: -15px;
  margin-left: -10px;
}
.show-part .intordut {
  margin-top: 30px;
  margin-bottom: 20px;
}
.show-part .show-text {
  padding: 10px;
  display: none;
}
.show-part .show-text h1 {
  font-weight: 700;
}
.show-part .show-text li {
  color: #6b6a6a;
  text-indent: 2em;
  padding: 2px;
}
.sellerlist-part {
  padding: 0 10%;
  background-color: #f8f8f8;
}
.sellerlist-part .container-fluid {
  background-color: white;
  border: 1px solid #dadada;
  border-top: 0px ;
}
.sellerlist-part .row {
  margin: 10px 0;
}
.sellerlist-part .thumbnail {
  padding: 8px;
  width: 100%;
  position: relative;
}
.sellerlist-part .leveIcon {
  width: 35px;
  height: 35px;
  position: absolute;
  top: 0;
  left: 0;
}
.scene-part .show-picture {
  padding-top: 20px;
}
.scene-part .show-picture div,
.scene-part .show-picture img {
  transform-origin: center;
  transform: scale(0, 0);
}
.scene-part .show-picture div {
  padding: 8px;
  border: 1px solid #6b6a6a;
  background-color: rgba(255, 255, 255, 0.5);
  -webkit-transition: all 0.75s ease;
  -moz-transition: all 0.75s ease;
  -o-transition: all 0.75s ease;
  transition: all 0.75s ease;
}
.scene-part .show-picture img {
  border: 1px solid #dadada;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.scene-part .imgList {
  padding-top: 10px;
  padding-bottom: 30px;
}
.scene-part .imgList .img-div {
  padding: 0;
  padding: 4px 4px;
  -webkit-transition: all 0.75s ease;
  -moz-transition: all 0.75s ease;
  -o-transition: all 0.75s ease;
  transition: all 0.75s ease;
  opacity: 0;
  transform: translateY(60px);
}
.scene-part .imgList .img-div a {
  -webkit-transition: all 0.4s ease;
  -moz-transition: all 0.4s ease;
  -o-transition: all 0.4s ease;
  transition: all 0.4s ease;
}
.scene-part .imgList .img-div .imgtitle {
  position: absolute;
  width: 100%;
  height: 30px;
  bottom: 20px;
  left: 0px;
  background-color: rgba(0, 0, 0, 0.4);
  -webkit-transition: all 0.4s ease;
  -moz-transition: all 0.4s ease;
  -o-transition: all 0.4s ease;
  transition: all 0.4s ease;
  z-index: -1;
  color: white;
  line-height: 30px;
  font-size: 20px;
  text-align: center;
  transform-origin: center ;
  transform: scaleX(0);
  opacity: 0;
}
@media only screen and (max-width: 640px) {
  .scene-part .imgList .img-div .imgtitle {
    z-index: 2;
    opacity: 1;
    transform: scaleX(1);
  }
}
.scene-part .imgList .img-div:hover a {
  transform: translateY(-35px);
  z-index: 40;
}
.scene-part .imgList img {
  width: 100%;
  vertical-align: middle;
}
.dress-part {
  margin-bottom: 20px;
}
.dress-part .show-picture {
  padding-top: 20px;
}
.dress-part .show-picture .big-img {
  -webkit-transition: all 0.8s ease;
  -moz-transition: all 0.8s ease;
  -o-transition: all 0.8s ease;
  transition: all 0.8s ease;
  transform: translateY(-500px);
  opacity: 0;
  position: relative;
}
.dress-part .show-picture .big-img .thumbnail {
  -webkit-box-shadow: -1px 0px 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: -1px 0px 6px rgba(0, 0, 0, 0.3);
  box-shadow: -1px 0px 6px rgba(0, 0, 0, 0.3);
}
.dress-part .show-picture .big-img img {
  height: 450px;
}
.dress-part .show-picture .big-img .litter-nav {
  position: absolute;
  width: 100px;
  height: 100%;
  right: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 20;
}
.dress-part .show-picture .big-img .litter-nav .active {
  color: white;
}
.dress-part .show-picture .big-img .litter-nav ul,
.dress-part .show-picture .big-img .litter-nav li {
  padding: 0;
}
.dress-part .show-picture .big-img .litter-nav li {
  list-style: none;
  padding-bottom: 5px;
  border-top: 1px solid white ;
  margin-left: 10px ;
}
.dress-part .show-picture .big-img .litter-nav a {
  color: rgba(255, 255, 255, 0.6);
}
.dress-part .show-picture .big-img .litter-nav img {
  text-align: center;
  display: block;
  width: 80%;
  height: 50px;
}
.dress-part .imgList {
  position: relative;
  display: none;
}
.dress-part .imgList .top-part {
  height: 300px;
  width: 100%;
}
.dress-part .imgList .top-part img {
  height: 100%;
}
.dress-part .imgList .top-part a {
  position: absolute;
  left: 0px;
  height: 300px;
  display: block;
  background-color: white;
  outline: 2px solid white;
  -webkit-transition: all 0.6s ease;
  -moz-transition: all 0.6s ease;
  -o-transition: all 0.6s ease;
  transition: all 0.6s ease;
  -webkit-box-shadow: -2px 0px 6px rgba(0, 0, 0, 0.5);
  -moz-box-shadow: -2px 0px 6px rgba(0, 0, 0, 0.5);
  box-shadow: -2px 0px 6px rgba(0, 0, 0, 0.5);
}
.dress-part .imgList .top-part a:hover {
  border: 1px solid #7d0022;
  z-index: 20;
  transform: scale(1.1, 1.1);
}
.dress-part .imgList .top-part a span {
  display: block;
  width: 100%;
  line-height: 35px;
  font-size: 20px;
  color: white;
  background-color: rgba(0, 0, 0, 0.4);
  text-align: center;
  position: absolute;
  bottom: 0;
}
.dress-part .imgList .bottom-part {
  margin-top: 50px;
  background-color: rgba(0, 0, 0, 0.4);
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.dress-part .imgList .bottom-part .produce .thumbnail {
  margin-top: 10px;
  width: 200px;
}
.dress-part .imgList .bottom-part h2,
.dress-part .imgList .bottom-part h3 {
  padding-left: 10px ;
  color: white;
}
.dress-part .imgList .bottom-part p {
  border-top: 1px solid white;
  text-indent: 2em;
  color: white;
  padding: 4px;
}
.dress-part .imgList .bottom-part .normal {
  height: 100px;
  margin-top: 50px;
}
#myModal .modal-img {
  width: 100%;
}
