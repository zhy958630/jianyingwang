* {
  margin: 0;
  padding: 0;
}
/*清除浮动*/
.clearfix:before,
.clearfix:after {
  content: "";
  display: block;
  clear: both;
}
/*字体倒影*/
.text-reflect{
	-webkit-box-reflect: below 10px -webkit-linear-gradient(transparent,transparent 50%,rgba(255,255,255,.5));
}
.header {
  width: 100%;
  height: 61px; 
}
.header .hbottom {
	position: relative;
	top: 40px;
  width: 1200px;
  margin: 0 auto;
}
.header .hbottom dl {
  width: 100%;
  height: 38px;
  line-height: 38px;
  border-bottom: 1px solid #e8e8e8;
  font-size: 12px;
}
.header .hbottom dl dt {
  float: left;
  line-height: 36px;
  margin-left: 1px;
  margin-right: 46px;
}
.header .hbottom dl dd {
  margin-right: 14px;
  float: left;
  line-height: 36px;
}
.header .hbottom dl dd span {
  color: #FFFFFF;
  width: 45px;
  height: 34px;
  text-align: center;
  float: left;
  display: block;
  background-color: #7ecef4;
}
.header .hbottom dl select {
  margin-right: 40px;
}
.header .hbottom dl .sum {
  color: #717171;
  float: right;
}
.main{
	width: 1215px;
	margin: 0 auto;
	position: relative;
	top:60px;
}
.main p{
	color: #6b6a6a;
	border-bottom: 1px solid #dadada;
}
.main p span{
	color: deepskyblue;
}
.main .grade{
	float: right;
}
.main .price{
	color: red;
	float: right;
}
.main .paging{
	margin: 0 auto;
	text-align: center;
	position: relative;
	top: 100px;
}
.main .paging li:nth-child(8){
	color: red;
	height: 30px;
	line-height: 30px;
}
.main .paging li:nth-child(8) div{
	
	display: inline-block;
	width: 35px;
	height: 35px;
	border: 1px solid #dadada;
}
.main .cameraman{
	margin-top: 160px;
	margin-bottom: 90px;
}
.main .cameraman h2{
	margin-bottom: 40px;
	color: white;
	text-shadow: 2px 2px 4px #000000; /*字体浮雕效果*/
}
.main .return{
		text-align: center;
		margin-bottom: 90px;
}
